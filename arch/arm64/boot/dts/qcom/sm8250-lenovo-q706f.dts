&tlmm {
	gpio-reserved-ranges = <40 4>;

	pri_tdm_active: pri-tdm-active-state {
		sck-pins {
			pins = "gpio138";
			function = "mi2s0_sck";
			drive-strength = <8>;
			bias-disable;
			output-high;
		};

		din-pins {
			pins = "gpio139";
			function = "mi2s0_data0";
			drive-strength = <8>;
			bias-disable;
		};

		ws-pins {
			pins = "gpio141";
			function = "mi2s0_ws";
			drive-strength = <8>;
			bias-disable;
			output-high;
		};

		dout-pins {
			pins = "gpio140";
			function = "mi2s0_data1";
			drive-strength = <8>;
			bias-disable;
		};
	};

	tert_tdm_active: tert-tdm-active-state {
		sck-pins {
			pins = "gpio133";
			function = "mi2s2_sck";
			drive-strength = <8>;
			bias-disable;
			output-high;
		};

		din-pins {
			pins = "gpio134";
			function = "mi2s2_data0";
			drive-strength = <8>;
			bias-disable;
		};

		ws-pins {
			pins = "gpio135";
			function = "mi2s2_ws";
			drive-strength = <8>;
			bias-disable;
			output-high;
		};

		dout-pins {
			pins = "gpio137";
			function = "mi2s2_data1";
			drive-strength = <8>;
			bias-disable;
		};
	};

	cam0_default: cam0-default-state {
		mclk0-pins {
			pins = "gpio94";
			function = "cam_mclk";

			drive-strength = <16>;
			bias-disable;
		};
	};

	spkr_1_sd_n_active: spkr-1-sd-n-active {
		pins = "gpio26";
		function = "gpio";
		drive-strength = <8>;
		bias-disable;
		output-high;
	};

	cs35l45_1_irq_default: cs35l45-1-irq-default {
		pins = "gpio24";
		function = "gpio";
		drive-strength = <0x02>;
		bias-pull-up;
		input-enable;
	};

	cs35l45_2_irq_default: cs35l45-2-irq-default {
		pins = "gpio51";
		function = "gpio";
		drive-strength = <0x02>;
		bias-pull-up;
	};

	cs35l45_3_irq_default: cs35l45-3-irq-default {
		pins = "gpio100";
		function = "gpio";
		drive-strength = <0x02>;
		bias-pull-up;
		input-enable;
	};

	cs35l45_4_irq_default: cs35l45-4-irq-default {
		pins = "gpio93";
		function = "gpio";
		drive-strength = <0x02>;
		bias-pull-up;
		input-enable;
	};

	cs35l45_reset_default: cs35l45-reset-default {
		pins = "gpio127";
		// drive-strength = <0x02>;
		// bias-pull-up;
		drive-strength = <8>;
		bias-disable;
		output-high;
	};

	cs35l45_1v8_enable: cs35l45-1v8-enable {
		pins = "gpio49";
		drive-strength = <0x10>;
		bias-disable;
		output-high;
	};

	bt_en_state: bt-default-state {
		pins = "gpio21";
		function = "gpio";
		drive-strength = <16>;
		output-low;
		bias-pull-up;
	};

	wlan_en_state: wlan-default-state {
		pins = "gpio20";
		function = "gpio";
		drive-strength = <16>;
		output-low;
		bias-pull-up;
	};

	panel_reset_pins: panel-reset-state {
		pins = "gpio75";
		function = "gpio";
		drive-strength = <8>;
		bias-disable;
		output-high;
	};

	panel_vsync_pins: panel-vsync-state {
		pins = "gpio66";
		function = "mdp_vsync";
		drive-strength = <2>;
		bias-pull-down;
	};

	panel_vout_pins: panel-vout-state {
		pins = "gpio24";
		function = "gpio";
		drive-strength = <16>;
		output-high;
	};

	sdc2_default_state: sdc2-default-state {
		clk-pins {
			pins = "sdc2_clk";
			drive-strength = <16>;
			bias-disable;
		};

		cmd-pins {
			pins = "sdc2_cmd";
			drive-strength = <16>;
			bias-pull-up;
		};

		data-pins {
			pins = "sdc2_data";
			drive-strength = <16>;
			bias-pull-up;
		};
	};

	ts_pwr_en: ts-pwr-en {
		pins = "gpio130";
		function = "gpio";
		drive-strength = <8>;
		bias-disable;
		output-high;
	};

	ts_irq: ts-irq-state {
		pins = "gpio31";
		function = "gpio";
		drive-strength = <8>;
		bias-pull-up;
	};

	ts_reset: ts-reset-state {
		pins = "gpio30";
		function = "gpio";
		drive-strength = <8>;
		bias-pull-up;
	};

	sd_pwr_en: sd-power {
		pins = "gpio114";
		function = "gpio";
		drive-strength = <8>;
		bias-disable;
		output-high;
	};

	sdc2_card_det_n: sd-card-det-n-state {
		pins = "gpio77";
		function = "gpio";
		bias-pull-up;
		drive-strength = <2>;
	};

	muic_i2c_default: muic-i2c-default-state {
		pins = "gpio12", "gpio13";
		function = "gpio";

		drive-strength = <2>;
		bias-disable;
	};
	wcd938x_reset_default: wcd938x-reset-default-state {
		pins = "gpio32";
		function = "gpio";
		drive-strength = <16>;
		output-high;
	};

	wcd938x_reset_sleep: wcd938x-reset-sleep-state {
		pins = "gpio32";
		function = "gpio";
		drive-strength = <16>;
		bias-disable;
		output-low;
	};
};
