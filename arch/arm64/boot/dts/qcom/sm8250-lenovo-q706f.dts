&i2c3 {
	status = "okay";
	clock-frequency = <400000>;
	/* hid-over-i2c Keyboard @ 61 */
	keyboard@61 {
        compatible = "hid-over-i2c";
		vdd-supply = <&vreg_l10c_3p0>;
        reg = <0x61>;
        hid-descr-addr = <0x01>;
		interrupt-parent = <&tlmm>;
		interrupts = <15 IRQ_TYPE_LEVEL_LOW>;
		// interrupts-extended = <&tlmm 15 IRQ_TYPE_LEVEL_LOW>;
